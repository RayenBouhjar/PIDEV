<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="utilisateur.AfficherAvisController">
   <children>
      <AnchorPane id="AnchorPane" prefHeight="717.0" prefWidth="703.0">
         <children>
            <TableView fx:id="table" layoutX="23.0" layoutY="50.0" onDragDetected="#handleMouseAction" onMouseClicked="#handleMouseAction" prefHeight="373.0" prefWidth="597.0">
               <columns>
                  <TableColumn fx:id="id" minWidth="8.0" prefWidth="11.0" text="ID" />
                  <TableColumn fx:id="note" prefWidth="93.0" text="Note" />
                  <TableColumn fx:id="nom" prefWidth="195.0" text="Nom" />
                  <TableColumn fx:id="description" prefWidth="261.0" text="Description" />
               </columns>
            </TableView>
            <Button layoutX="397.0" layoutY="454.0" mnemonicParsing="false" onAction="#archiver" text="Archiver" />
            <Button layoutX="501.0" layoutY="454.0" mnemonicParsing="false" onAction="#supprimer" text="Supprimer" />
            <PieChart fx:id="pie" layoutX="23.0" layoutY="454.0" prefHeight="232.0" prefWidth="375.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
