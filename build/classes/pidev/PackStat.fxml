<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="426.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pidev.PackStatController">
   <children>
      <BarChart fx:id="barchart" layoutX="50.0" layoutY="50.0" prefHeight="362.0" prefWidth="500.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxis" side="LEFT" />
        </yAxis>
      </BarChart>
      <Label layoutX="213.0" layoutY="14.0" text="Pack Price per Quantity">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Button fx:id="dismiss" layoutX="14.0" layoutY="15.0" mnemonicParsing="false" onAction="#Cancel" text="Retour" />
   </children>
</AnchorPane>
